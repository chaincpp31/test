*,*::before,*::after {
	padding: 0;
	margin: 0;
}

i {
	margin-right: .5em;
}

.head {
	background-color : #000055;
	color : #000033;
}

h1 {
		margin-top: 1em;
    color: red;
}

p b {
	margin-top: 1em;
}

body {
	font-family: sans-serif;
}

.carousel {
  background : none ;
}

.carousel-cell {
  width: 66%;
  height: 200px;
  margin-right: 10px ;
  background-image: url(pic/logo.png);
  border-radius: 5px;
  counter-increment: gallery-cell;
}

.img {
    margin: 5px;
    border: 1px solid #ccc;
    float: left;
    width: 180px;
}

.img:hover {
    border: 1px solid #777;
}

.img img {
    width: 100%;
    height: auto;
}

.desc {
    padding: 15px;
    text-align: center;
}

ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    background-color: #000055;
}

li {
    float: left;
    border-right:1px solid #bbb;
}

li:last-child {
    border-right: none;
}

li a {
	display: block;
	color: white;
	text-align: center;
	padding: 14px 16px;
	text-decoration: none;
	background-color:#000055;
}

li a:hover:not(.active) {
    background-color: #B5B5FD;
}

.active {
    background-color: #111;
}

.about{
	background-color : #D1EEEE ;
	padding : 5px 5px 5px 5px;
	border : 0 0 25px 0 solid black;
	font-size : 14px;
}

.register{
	text-align: left;
	padding: 5px 0px 5px 5px;
	border: 2px solid black;
	margin: 6px;
}

.menu__btn {
  background-color: #4CAF50;
  color: white;
  padding: 16px;
  font-size: 16px;
  border: none;
  cursor: pointer;
	min-width: 160px;
}

.menu {
  position: relative;
  display: inline-block;
	float : right;
}

.smenu {
	min-width: 160px;
	background-color: #4CAF50;
	content: '';
  position: absolute;
	left: 0;
	top: 100%;
	max-height: 0;
	overflow: hidden;
	transition: all ease-in-out 300ms;
	z-index: 10;
}

.smenu a {
	min-width: 160px;
  color: white;
  padding: 12px 16px;
  text-decoration: none;
  display: block;
}

.smenu a:hover {
	background-color: #99FFCC;
}

.menu:hover .smenu {
  max-height: 10em;
}

.menu:hover .menu__btn {
  background-color: #B5B5FD;
}
